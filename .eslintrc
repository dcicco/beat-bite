{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": "off",
    "vars-on-top": "off",
    "quotes": "warn",
    "no-var": "off",
    "prefer-arrow-callback": "warn",
    "eqeqeq": "warn",
    "radix": "off",
    "no-console": "off",
    "no-undef": "off",
    "quote-props": "off",
  }
}
